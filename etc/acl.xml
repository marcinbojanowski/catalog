<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="MB_Catalog::product" title="MB Catalog Product" sortOrder="10">
                    <resource id="MB_Catalog::product_save" title="Save Product" sortOrder="10"/>
                    <resource id="MB_Catalog::product_delete" title="Delete Product" sortOrder="20"/>
                    <resource id="MB_Catalog::product_update" title="Update Product" sortOrder="30"/>
                    <resource id="MB_Catalog::product_view" title="View Product" sortOrder="40"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
